{
  "navigationFallback": {
    "rewrite": "/taskpane.html",
    "exclude": ["/assets/*", "/*.js", "/*.css", "/*.png", "/*.ico", "/api/*", "/*.xml"]
  },
  "routes": [
    {
      "route": "/taskpane.html",
      "rewrite": "/taskpane.html"
    },
    {
      "route": "/manifest.azure.xml",
      "headers": {
        "Content-Type": "application/xml"
      }
    },
    {
      "route": "/assets/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/taskpane.html"
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".xml": "application/xml"
  }
}
