{
  "name": "outlook-ai-assistant",
  "version": "1.5.0",
  "description": "AI-powered Outlook Add-in for email management, calendar scheduling, and task automation",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "npm run build:frontend && npm run build:server",
    "build:frontend": "webpack --mode production",
    "build:server": "tsc -p tsconfig.server.json",
    "build:dev": "webpack --mode development",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "webpack serve --mode development --port auto",
    "dev:server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "start": "node dist/server/index.js",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "stop:desktop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "validate:prod": "office-addin-manifest validate manifest.production.xml",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "deploy:prep": "npm run build && echo 'Ready for deployment!'"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.0",
    "@azure/msal-browser": "^5.0.2",
    "@fluentui/react": "^8.110.0",
    "@fluentui/react-components": "^9.46.0",
    "@microsoft/office-js": "^1.1.86",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "openai": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@babel/preset-typescript": "^7.23.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.9.0",
    "@types/office-js": "^1.0.377",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "babel-loader": "^9.1.3",
    "canvas": "^3.2.1",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "html-webpack-plugin": "^5.5.3",
    "office-addin-debugging": "^5.0.0",
    "office-addin-dev-certs": "^2.0.6",
    "office-addin-manifest": "^1.12.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
